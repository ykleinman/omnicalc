<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Descriptive Statistics Results</h3>
  </div>
  <div class="panel-body">
    <dl class="dl-horizontal">
      <dt>Numbers</dt>
      <dd><%= @numbers %></dd>

      <dt>Count</dt>
      <dd><%= @count %></dd>

      <dt>Minimum</dt>
      <dd><%= @minimum %></dd>

      <dt>Maximum</dt>
      <dd><%= @maximum %></dd>

      <dt>Range</dt>
      <dd><%= @range %></dd>

      <dt>Median</dt>
      <dd><%= @median %></dd>

      <dt>Sum</dt>
      <dd><%= @sum %></dd>

      <dt>Mean</dt>
      <dd><%= @mean %></dd>

      <dt>Variance</dt>
      <dd><%= @variance %></dd>

      <dt>Standard Deviation</dt>
      <dd><%= @standard_deviation %></dd>

      <dt>Mode</dt>
      <dd><%= @mode %></dd>
    </dl>
  </div>
</div>
